{
    "model_parameters": {
        "hidden_layer_size": 16,
        "dropout_rate": 0.2,
        "input_sequence_length": 13,
        "target_sequence_length": 15,
        "epochs": 60,
        "attention_head_size":4,

        "optimizer": "adam",
        "learning_rate": 0.001,
